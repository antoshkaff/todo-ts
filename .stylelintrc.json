{
  "extends": [
    "stylelint-config-standard"
  ],

  "rules": {
    "selector-class-pattern": null,
    "media-feature-range-notation": "prefix",
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind", "apply", "responsive", "screen", "mixin", "include", "function"]
      }
    ]
  }
}
